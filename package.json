{
  "name": "theme-vdoing-blog",
  "version": "1.0.0",
  "scripts": {
    "dev": "vuepress dev docs",
    "build": "node --max-old-space-size=4096 --openssl-legacy-provider ./node_modules/.bin/vuepress build docs",
    "deploy": "bash deploy.sh",
    "autoGitTalk": "node utils/genSiteMap.js && node utils/autoInitGitTalk.js",
    "editFm": "node utils/editFrontmatter.js",
    "baiduPush": "node utils/baiduPush.js https://9zhe.tech && bash baiduPush.sh",
    "publish": "cd theme-vdoing && npm publish && cd ../ && npm uninstall vuepress-theme-vdoing && npm i vuepress-theme-vdoing -D"
  },
  "license": "MIT",
  "devDependencies": {
    "inquirer": "^7.1.0",
    "json2yaml": "^1.1.0",
    "moment": "^2.25.3",
    "request": "^2.88.2",
    "vuepress": "1.5.2",
    "vuepress-plugin-baidu-autopush": "^1.0.1",
    "vuepress-plugin-baidu-tongji": "^1.0.1",
    "vuepress-plugin-demo-block": "^0.7.2",
    "vuepress-plugin-one-click-copy": "^1.0.2",
    "vuepress-plugin-thirdparty-search": "^1.0.2",
    "vuepress-plugin-zooming": "^1.1.7",
    "vuepress-theme-vdoing": "^1.4.1",
    "yamljs": "^0.3.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "vuepress-plugin-comment": "^0.7.3",
    "vuepress-plugin-sitemap": "^2.3.1",
    "xml-parser": "^1.2.1"
  }
}
